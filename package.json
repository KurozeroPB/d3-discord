{
    "name": "slash-create-template",
    "version": "1.0.0",
    "description": "A template for slash-create",
    "license": "MIT",
    "main": "dist/index.js",
    "scripts": {
        "sync": "slash-up sync",
        "sync:dev": "slash-up sync -e development",
        "start": "cd dist && cross-env NODE_ENV=production node index.js",
        "test": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./src/index.ts",
        "build": "npx tsc",
        "build:test": "npx tsc --noEmit",
        "clean": "(if exist \"node_modules\" rd /q /s node_modules) && (if exist \"dist\" rd /q /s dist) && (if exist \"yarn.lock\" del /q yarn.lock)",
        "lint": "npx eslint --ext .ts ./src",
        "lint:fix": "npx eslint --ext .ts ./src --fix"
    },
    "dependencies": {
        "@kurozero/collection": "^0.5.1",
        "axios": "^0.26.1",
        "bufferutil": "^4.0.6",
        "chalk": "4.1.2",
        "dotenv": "^16.0.0",
        "eris": "^0.16.1",
        "eris-additions": "^1.4.1",
        "eris-pagination": "^0.5.2",
        "moment": "^2.29.3",
        "mongoose": "^6.3.1",
        "os-utils": "^0.0.14",
        "slash-create": "^5.2.0",
        "winston": "^3.7.2",
        "yukikaze": "^1.0.0"
    },
    "devDependencies": {
        "@types/mongoose": "^5.11.97",
        "@types/node": "^17.0.25",
        "@typescript-eslint/eslint-plugin": "^5.20.0",
        "@typescript-eslint/parser": "^5.20.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.6.2",
        "slash-up": "^1.1.0",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.3"
    }
}
